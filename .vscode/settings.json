{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "baphy/__init__.py": true,
    "baphy/__pycache__": true,
    ".vscode": true,
    "tests/__init__.py": true,
    ".venv": true,
    "baphy/src/__pycache__": true,
    "baphy/src/queries/__pycache__": true,
    "baphy/src/queries/__init__.py": true,
    "tests/tests_queries/tests_project_info/__pycache__": true,
    "tests/test_example/__pycache__": true,
    ".pytest_cache": true
  },
  "hide-files.files": [
    "baphy/__init__.py",
    "baphy/__pycache__",
    ".vscode",
    "tests/__init__.py",
    ".venv",
    "baphy/src/__pycache__",
    "baphy/src/queries/__pycache__",
    "baphy/src/queries/__init__.py",
    "tests/tests_queries/tests_project_info/__pycache__",
    "tests/test_example/__pycache__",
    ".pytest_cache"
  ],
  "python.analysis.extraPaths": [
    "./baphy/src",
    "./baphy"
  ]
}
